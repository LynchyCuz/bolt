  }
  $('body').on('click','#sticky_btn_add button',function(e){
    	e.preventDefault();
    	$('#sticky_btn_add button').attr('disabled',true);
    	setTimeout(function(){
          	$('#sticky_btn_add button').text('Adding...');
        	$('.hidden-sm-down .product-details .sqs-add-to-cart-button').trigger('click');
     		console.log('sticky btn clicked');
          setTimeout(function(){
          	$('#sticky_btn_add button').text('Added!');
          	setTimeout(function(){
              console.log('sticky btn enabled');
              $('#sticky_btn_add button').text('Add To Cart');
              $('#sticky_btn_add button').attr('disabled',false);
            },3000);
          },1000);
        },1000);
   });
</script>





