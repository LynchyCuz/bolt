<script>
  document.addEventListener("DOMContentLoaded", function () {
    if (window.location.hash === "#669f143577e2cb28265865cf") {
      // Delay the scroll to the section to prevent instant scrolling
      setTimeout(function () {
        const targetElement = document.getElementById("669f143577e2cb28265865cf");
        if (targetElement) {
          targetElement.scrollIntoView({ behavior: "smooth" });
        }
      }, 1000); // Adjust delay (100ms) as needed
    }
  });
 document.addEventListener("DOMContentLoaded", function () {
    // Get the current URL path
    const currentPath = window.location.pathname;

    // Hide the button if the path matches
    if (currentPath === "/order/p/carewindow-videophone") {
      const enquireButton = document.querySelector(".sticky-enquire-btn");
      if (enquireButton) {
        enquireButton.style.display = "none";
      }
    }
  });
</script>



<!-- solution 2: side sliding inquiry form-->
<div id="inquiry-section-container">
  <button class="close-button" onclick="closeForm()">✖</button>
  <iframe id="slidingForm" src="./CareWindow _ Easy to Use Video Phone for Seniors_files/inquiry-form.html" width="100%" height="100%" frameborder="0"></iframe>
</div>

<script>
function scrollToEnquireSection(event) {
    if (event) {
        event.preventDefault();
    }
    const container = document.getElementById('inquiry-section-container');
    container.style.display = 'block';
    // Small delay to ensure display: block has taken effect
    requestAnimationFrame(() => {
        requestAnimationFrame(() => {
            container.classList.add('active');
        });
    });
}

function closeForm() {
    const container = document.getElementById('inquiry-section-container');
    container.classList.remove('active');
    
    // Wait for the sliding animation to complete before hiding
    setTimeout(() => {
        container.style.display = 'none';
    }, 200); // Match the transition duration
}

// Add click event to the enquire button
document.addEventListener('DOMContentLoaded', function() {
    const enquireButton = document.querySelector('.enquire-button');
    if (enquireButton) {
        enquireButton.addEventListener('click', scrollToEnquireSection);
    }
});
</script>







<!-- ANNOUNCEMENT BAR CODE INJECTION -->
<div id="announcementBar" class="announcement-bar">
  <div class="container">
    <div class="content">
      <button class="nav-button" aria-label="Previous announcement">
        <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
          <polyline points="15 18 9 12 15 6"></polyline>
        </svg>
      </button>
      <div class="announcement-text">
        <div class="announcement-content animate-swipe swipe-left">30 day satisfaction guarantee 🌟</div>
      </div>
      <button class="nav-button" aria-label="Next announcement">
        <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
          <polyline points="9 18 15 12 9 6"></polyline>
        </svg>
      </button>
    </div>
  </div>
</div>

<script>
  class AnnouncementBar {
    constructor() {
      this.announcements = [
        "30 day satisfaction guarantee 🌟",
        "Fast delivery in AU & NZ 🚚",
        "Private and secure 🔒"
      ];
      this.currentIndex = 0;
      this.isVisible = true;
      this.isNext = true; // Flag to determine swipe direction

      // Get DOM elements
      this.bar = document.getElementById('announcementBar');
      this.content = this.bar.querySelector('.announcement-content');
      const [prevBtn, nextBtn] = this.bar.querySelectorAll('.nav-button');

      // Bind event listeners
      prevBtn.addEventListener('click', () => this.handlePrevious());
      nextBtn.addEventListener('click', () => this.handleNext());

      // Initialize content
      this.updateContent();

      // Start the automatic swipe every 6 seconds
      this.startAutoSwipe();
    }

    updateContent() {
      this.content.classList.remove('swipe-left', 'swipe-right'); // Remove existing animation classes

      // Trigger reflow to reset animation
      void this.content.offsetWidth;

      this.content.textContent = this.announcements[this.currentIndex];

      // Add the swipe animation based on the direction of the change
      if (this.isNext) {
        this.content.classList.add('swipe-left');
      } else {
        this.content.classList.add('swipe-right');
      }
    }

    handlePrevious() {
      this.isNext = false;
      this.currentIndex = (this.currentIndex - 1 + this.announcements.length) % this.announcements.length;
      this.updateContent();
    }

    handleNext() {
      this.isNext = true;
      this.currentIndex = (this.currentIndex + 1) % this.announcements.length;
      this.updateContent();
    }

    startAutoSwipe() {
      this.autoSwipeTimer = setInterval(() => {
        this.handleNext(); // Automatically trigger the "Next" button click
      }, 6000); // Set the interval to 6 seconds
    }

    stopAutoSwipe() {
      if (this.autoSwipeTimer) {
        clearInterval(this.autoSwipeTimer); // Stop the automatic swipe when it's no longer needed
      }
    }
  }

  // Initialize the announcement bar when the DOM is loaded
  document.addEventListener('DOMContentLoaded', () => {
    new AnnouncementBar();
  });
</script>



<!-- ADD TO CART STICKY V2.0 -->
<div id="sticky-cart" class="sticky-cart">
  <div class="sticky-cart-container">
    <div class="quantity-controls">
      <input aria-label="Quantity" size="4" max="9999" min="1" value="1" type="number" step="1" class="quantity-input">
    </div>
    <div class="add-to-cart-btn-container"> 
    <button class="add-to-cart-btn">
      <span>Add to Cart</span>
    </button>
  </div>
  </div>
</div>


<script>
// Check if we're on the correct page
if (window.location.pathname === '/order/p/carewindow-videophone') {
  // Create and add the sticky cart to the page
  const stickyCartHTML = document.getElementById("sticky-cart").outerHTML;
  document.body.insertAdjacentHTML("beforeend", stickyCartHTML);
  document.getElementById("sticky-cart").remove();

  // Initialize variables
  let quantity = 1;
  let lastScrollY = 0;
  const stickyCart = document.querySelector(".sticky-cart");
  const quantityInput = stickyCart.querySelector(".quantity-input");
  const addToCartBtn = stickyCart.querySelector(".add-to-cart-btn");

  stickyCart.style.display = 'block';
  
  // Handle scroll behavior
  function handleScroll() {
    if (window.innerWidth > 768) {
      const currentScrollY = window.scrollY;
      if (currentScrollY > lastScrollY && currentScrollY > 200) {
        stickyCart.classList.add("hidden");
      } else {
        stickyCart.classList.remove("hidden");
      }
      lastScrollY = currentScrollY;
    }
  }

  // Function to update all quantity inputs on the page
  function updateAllQuantityInputs(value) {
    // Update sticky cart quantity
    quantityInput.value = value;
    
    // Update main product quantity inputs
    const productQuantityInputs = document.querySelectorAll('.product-quantity-input input[aria-label="Quantity"]');
    productQuantityInputs.forEach(input => {
      input.value = value;
      
      // Create and dispatch an input event to trigger any listeners
      const inputEvent = new Event('input', { bubbles: true });
      input.dispatchEvent(inputEvent);
      
      // Create and dispatch a change event to trigger any listeners
      const changeEvent = new Event('change', { bubbles: true });
      input.dispatchEvent(changeEvent);
    });
  }

  // Handle quantity input changes
  quantityInput.addEventListener("input", (event) => {
    const newQuantity = parseInt(event.target.value, 10);
    quantity = isNaN(newQuantity) || newQuantity < 1 ? 1 : newQuantity;
    updateAllQuantityInputs(quantity);
  });

  // Handle add to cart
  function handleAddToCart() {
    // Update quantity first
    updateAllQuantityInputs(quantity);
    
    // Small delay to ensure quantity is updated before clicking
    setTimeout(() => {
      const addToCartButton = document.querySelector(".sqs-add-to-cart-button");
      if (addToCartButton) {
        addToCartButton.click();
      }
    }, 100);
  }

  // Add event listeners
  window.addEventListener("scroll", handleScroll, { passive: true });
  addToCartBtn.addEventListener("click", handleAddToCart);

  // Sync quantities initially
  updateAllQuantityInputs(quantity);
}
